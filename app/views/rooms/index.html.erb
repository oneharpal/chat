<p style="color: green"><%= notice %></p>
<h1>Rooms</h1>

<div id="rooms">
  <% @rooms.each do |room| %>
    <%= render room %>
    <p>
      <%= link_to "Show", room %>
      <%= link_to "edit", edit_room_path(room) %>
      <%= link_to "delete", room_path(room), method: :delete %>
    </p>
  <% end %>
</div>

<%= link_to "New room", new_room_path %>
